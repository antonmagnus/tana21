N = 5;
A = full(gallery('tridiag',N,-1,2.1,-1));
known_x = ones(N,1); % make known solution vector of 1s
manuf_b = A*known_x; % manufacture the right-hand-side
tol = 10e-11;
save = zeros(2,1000);
c = 1;
for w = 1:0.001:2
    [~,k] = sor(A,manuf_b,zeros(N,1),w,tol);
    save(1,c) = k;
    save(1,c) = k;
    c = c+1;
end
